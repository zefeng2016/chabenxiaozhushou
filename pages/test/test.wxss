.body {
  background: #2e3b58;
  position: absolute;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  padding: 20rpx;
  margin: 0px;

  color: white;
  font-size: 18pt;
}

.nextBtn {
  text-align: center;
  margin-top: 80rpx;
  width: 600rpx;
}

.trueNum {
